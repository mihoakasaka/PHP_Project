{# empty Twig template #}
<div >
 
        {% for a in adsList %}
            <div class="adContainer">
                <div class="imageContainer">
                 <a href="/ad/{{a.id}}">   <img src="{{a.imagePath}}" width="100"></a>
                </div>
                <div class="title">
                    <p>{{a.title}}</p>
                </div>
                <div class="price">
                    <p>{{a.price}}</p>      
                </div>

            </div>                   
        {% endfor %}

    </div>